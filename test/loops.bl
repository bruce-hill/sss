repeat
    stop

nums := [10,20,30,40]
tmp := 1
while tmp <= #nums
    say "$(nums[tmp])", end=""
    tmp += 1
between say ", ", end=""
say ""

for i,num in nums
    say "[$i]$num", end=""
between say ", ", end=""
say ""

say "$i" for i in 100..102

extern random():Int

def retry():Bool
    r := random()
    say "rng = $r"
    return r mod 4 != 0

say "doop\n" while retry()

for i in [1,-2,3,-4,5]
    skip if i < 0
    say "i = $i"
between
    say "---"

for outer in [-10,1,2,-2,3000,4]
    skip if outer < 0
    for inner in [10,-1,20,-3,30,40,100,15]
        skip if inner < 0
        stop outer if inner*outer > 1000
        while yes
            say "inner: $inner outer: $outer product: $(inner*outer)"
            stop while
        between say "Between while"
        say "Finished inner"
    between say "Between inner"
    say "Finished outer"
between say "Between outer"
